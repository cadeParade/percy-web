{
  "name": "percy-web",
  "version": "1.0.0",
  "description": "Percy web application",
  "repository": "percy/percy-web",
  "license": "MIT",
  "author": "Perceptual Inc.",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "build-test": "ember build --environment=test -o dist",
    "build-production": "yarn run clean && EMBER_ENV=production ember build --environment=production",
    "clean": "rm -rf dist/ tmp/",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "lint:fix": "./node_modules/eslint/bin/eslint.js . --fix",
    "postinstall": "bower install --allow-root",
    "start": "ember server",
    "test": "ember test",
    "start-production-for-testing": "ember server --environment=production",
    "test:node_debug": "node debug node_modules/ember-cli/bin/ember test --server",
    "test:parallel": "EMBER_ENV=test percy exec --network-idle-timeout 50 -- ember exam --environment=test --path dist --split=4 --parallel",
    "test:server": "ember test --server"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/eslint/bin/eslint.js --fix",
      "git add"
    ]
  },
