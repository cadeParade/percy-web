{{title this.model.build.buildTitle}}
<BuildContainer
  @build={{this.model.build}}
  @isSnapshotsLoading={{this.isSnapshotsLoading}}
  @defaultBrowser={{this.defaultBrowser}}
  @allChangedBrowserSnapshotsSorted={{this.allChangedBrowserSnapshotsSorted}}
  @initializeSnapshotOrdering={{route-action "initializeSnapshotOrdering"}}
  @isUnchangedSnapshotsVisible={{this.isUnchangedSnapshotsVisible}}
  @isBuildApprovable={{this.isBuildApprovable}}
  @notifyOfUnchangedSnapshots={{action "notifyOfUnchangedSnapshots"}}
  @metadataSort={{this.metadataSort}}
/>
{{outlet}}
