<Build2::SnapshotViewer
  @build={{this.snapshot.build}}
  @snapshot={{this.snapshot}}
  @snapshotSelectedWidth={{this.snapshotSelectedWidth}}
  @comparisonMode={{this.comparisonMode}}
  @activeBrowser={{this.activeBrowser}}
  @isBuildApprovable={{this.isBuildApprovable}}
  @createReview={{route-action "noop"}}
  @goToPreviousSnapshot={{route-action "goToPreviousSnapshot"}}
  @goToNextSnapshot={{route-action "goToNextSnapshot"}}
  {{!-- @toggleSnapshotListVisibility={{route-action "toggleSnapshotListVisibility"}} --}}
  @createCommentThread={{route-action "noop"}}
  @closeCommentThread={{route-action "noop"}}
  @createComment={{route-action "noop"}}
/>
