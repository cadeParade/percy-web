{{#each this.page.blocks as |block|}}
  {{#if block.isContentBlock}}
    <Marketing::ContentBlock @block={{block.contentBlock}} />
  {{else if block.isVideoBlock}}
    <Marketing::VideoBlock @block={{block.videoBlock}} />
  {{else if block.isQuoteBlock}}
    <Marketing::CustomerQuoteBlock @quoteBlock={{block.quoteBlock}} />
  {{else if block.isFooter}}
    <Marketing::FooterCta @content={{block.footerCta}} />
  {{else if block.isHero}}
    <Marketing::MarketingHeader
      @logomark={{block.hero.logomark}}
      @image={{block.hero.mainImage}}
      @classes={{block.hero.classes}}
    >
      <Marketing::HeroBlock @content={{block.hero}} />
    </Marketing::MarketingHeader>
  {{else if block.isFaqBlock}}
    <Marketing::FaqBlock @block={{block.faqBlock}} />
  {{else if block.isLogoBlock}}
    <Marketing::LogoBlock @logos={{block.logoBlock.customerLogos}} />
  {{else if block.isProfileBlock}}
    <Marketing::ProfileBlock @profiles={{block.profileBlock.profiles}} />
  {{else if block.isPricingTableBlock}}
    <Marketing::PricingTable @table={{block.pricingTableBlock}} />
  {{else if block.isPricingCardBlock}}
    <Marketing::PricingCardBlock />
  {{else if block.isCaseStudyBlock}}
    <Marketing::CaseStudyBlock @block={{block.caseStudyBlock}} />
  {{/if}}
{{/each}}
