<table data-test-commit-table>
  <tbody>
    <tr>
      <td>Commit</td>
      <td>
        {{#if isRepoLinked}}
          <code><a data-test-commit-table-branch-url href="{{build.branchHtmlUrl}}">{{build.branch}}</a></code>
          @
          <a data-test-commit-table-commit-url href="{{build.commitHtmlUrl}}">{{commit.shaShort}}</a>
        {{else}}
          <code>{{build.branch}}</code> @ <code>{{commit.shaShort}}</code>
        {{/if}}
      </td>
    </tr>
    <tr><td>Committer</td><td>{{commit.authorName}}</td></tr>
    <tr><td>Message</td><td class="BuildInfoDropdown-commitMessage">{{commit.message}}</td></tr>
  </tbody>
</table>
