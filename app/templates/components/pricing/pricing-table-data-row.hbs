<td class="font-semibold p-1 pl-2">{{this.rowTitle}}</td>
<td class="text-center text-gray-500 p-1">
  {{#if this.consolidate}}
    {{get this.plans.startupPlans.firstObject this.field}}
  {{else}}
    {{#each this.plans.startupPlans as |plan|}}
      <div><strong>{{plan.name}}</strong>: {{get plan this.field}}</div>
    {{/each}}
  {{/if}}
</td>
<td class="text-center text-gray-500 p-1">{{get this.plans.businessPlan this.field}}</td>
<td class="text-center text-gray-500 p-1">{{get this.plans.enterprisePlan this.field}}</td>

